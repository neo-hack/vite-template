    "react-dom": "^17.0.2",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1",
    "tailwindcss": "^3.0.13"
  },
  "devDependencies": {
    "@aiou/eslint-config": "^0.4.0",
    "@changesets/cli": "^2.16.0",
    "@types/react": "^17.0.33",
